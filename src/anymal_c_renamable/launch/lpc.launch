  <!-- Locomotion PC stack -->
  <include file="$(find stack_launcher)/launch/stack_launcher.launch">
    <arg name="stack"         value="lpc"/>
    <arg name="trigger_param" value="/config_loaded"/>
  </include>

</launch>
