<?xml version="1.0" encoding="UTF-8"?>
<package format="2">
  <name>rqt_rviz_wrapper</name>
  <version>0.5.1</version>
  <description>rqt_rviz_wrapper provides a GUI plugin to embed a RViz window.</description>

  <author email="sbachmann@anybotics.com">Samuel Bachmann</author>
  <maintainer email="sbachmann@anybotics.com">Samuel Bachmann</maintainer>
  <maintainer email="rdiethelm@anybotics.com">Remo Diethelm</maintainer>
  <license>Proprietary</license>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>cmake_clang_tools</build_depend>

  <depend>message_logger</depend>
  <depend>qtbase5-dev</depend>
  <depend>rqt_gui</depend>
  <depend>rqt_gui_cpp</depend>
  <depend>signal_handler</depend>
  <depend>spinner_widget</depend>
  <depend>wmctrl</depend>

  <export>
    <rqt_gui plugin="${prefix}/plugin.xml"/>
  </export>
</package>
