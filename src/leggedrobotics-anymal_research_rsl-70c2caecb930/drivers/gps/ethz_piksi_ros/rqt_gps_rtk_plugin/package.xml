<?xml version="1.0"?>
<package format="2">
  <name>rqt_gps_rtk_plugin</name>
  <version>1.10.0</version>
  <description>The rqt_gps_rtk_plugin package</description>

  <author email="marcot@ethz.ch">Marco Tranzatto</author>
  <maintainer email="ynava@anybotics.com">Yoshua Nava</maintainer>
  <maintainer email="mwulf@anybotics.com">Maximilian Wulf</maintainer>

  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>
  <depend>rqt_gui</depend>
  <depend>rqt_gui_cpp</depend>
  <depend>piksi_rtk_msgs</depend>
  <depend>sensor_msgs</depend>

  <depend>libqt5-gui</depend>
  <depend>libqt5-widgets</depend>

  <build_export_depend>qtbase5-dev</build_export_depend>

  <!-- The export tag contains other, unspecified, tags -->
  <export>
    <architecture_independent/>
    <rqt_gui plugin="${prefix}/plugin.xml"/>
  </export>
</package>
