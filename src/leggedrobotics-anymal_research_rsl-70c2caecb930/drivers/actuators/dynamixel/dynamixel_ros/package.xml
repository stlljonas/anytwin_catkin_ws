<?xml version="1.0"?>
<package format="2">
  <name>dynamixel_ros</name>
  <version>0.3.0</version>
  <description>Dynamixel ROS Node</description>
  <author email="jerome.maye@mavt.ethz.ch">Jerome Maye</author>
  <maintainer email="rdiethelm@anybotics.com">Remo Diethelm</maintainer>
  <maintainer email="sbachmann@anybotics.com">Samuel Bachmann</maintainer>
  <license>Proprietary</license>

  <buildtool_depend>catkin</buildtool_depend>

  <depend>diagnostic_updater</depend>
  <depend>dynamixel_ros_msgs</depend>
  <depend>libdynamixel</depend>
  <depend>param_io</depend>
  <depend>rosbash</depend>
  <depend>roscpp</depend>
  <depend>rospy</depend>
  <depend>sensor_msgs</depend>
  <depend>signal_handler</depend>
  <depend>std_msgs</depend>
  <depend>std_srvs</depend>

  <export>
    <nodelet plugin="${prefix}/nodelet_plugins.xml"/>
  </export>
</package>
