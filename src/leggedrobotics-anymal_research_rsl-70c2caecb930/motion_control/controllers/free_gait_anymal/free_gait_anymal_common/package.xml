<?xml version="1.0"?>
<package format="2">
  <name>free_gait_anymal_common</name>
  <version>0.6.0</version>
  <description>Common Free Gait modules for the ANYmal robot.</description>
  <maintainer email="fgiraldez@anybotics.com">Francisco Giráldez Gámez</maintainer>
  <maintainer email="vyuryev@anybotics.com">Valentin Yuryev</maintainer>
  <license>Proprietary</license>
  <author email="pfankhauser@anybotics.com">Peter Fankhauser</author>
  <buildtool_depend>catkin</buildtool_depend>
  <depend>any_measurements</depend>
  <depend>any_measurements_ros</depend>
  <depend>free_gait_core</depend>
  <depend>free_gait_ros</depend>
  <depend>geometry_utils</depend>
  <depend>geometry_utils_ros</depend>
  <depend>grid_map_core</depend>
  <depend>grid_map_ros</depend>
  <depend>anymal_model</depend>
  <depend>anymal_model_ros</depend>
  <test_depend>anymal_b</test_depend>
  <test_depend>rostest</test_depend>
  <export>
  	<free_gait_core plugin="${prefix}/plugin_description.xml" />
  	<free_gait_ros plugin="${prefix}/plugin_ros_description.xml" />
  </export>
</package>
