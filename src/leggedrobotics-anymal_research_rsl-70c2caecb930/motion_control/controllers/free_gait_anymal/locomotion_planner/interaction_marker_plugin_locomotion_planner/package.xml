<?xml version="1.0"?>

<package format="2">

  <name>interaction_marker_plugin_locomotion_planner</name>
  <version>0.4.0</version>
  <description>Interactive marker plugin for publishing goal poses for locomotion planner.</description>

  <license>Proprietary</license>

  <author email="vyuryev@anybotics.com">Valentin Yuryev</author>
  <maintainer email="vyuryev@anybotics.com">Valentin Yuryev</maintainer>
  <maintainer email="fgiraldez@anybotics.com">Francisco Giráldez Gámez</maintainer>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>cmake_clang_tools</build_depend>

  <depend>interaction_marker</depend>
  <depend>locomotion_planner_msgs</depend>
  <depend>message_logger</depend>
  <depend>actionlib</depend>

  <export>
    <interaction_marker plugin="${prefix}/interaction_marker_plugin.xml"/>
  </export>

</package>
